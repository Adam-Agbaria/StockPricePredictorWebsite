{
  "buildCommand": "echo 'No build step required for static site'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/web_app/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/web_app/index.html"
    },
    {
      "source": "/predictor.html",
      "destination": "/web_app/predictor.html"
    },
    {
      "source": "/realtime.html",
      "destination": "/web_app/realtime.html"
    },
    {
      "source": "/styles.css",
      "destination": "/web_app/styles.css"
    },
    {
      "source": "/app.js",
      "destination": "/web_app/app.js"
    },
    {
      "source": "/realtime.js",
      "destination": "/web_app/realtime.js"
    },
    {
      "source": "/sample_data.json",
      "destination": "/web_app/sample_data.json"
    },
    {
      "source": "/historical_predictions.json",
      "destination": "/web_app/historical_predictions.json"
    },
    {
      "source": "/tfjs_model/(.*)",
      "destination": "/tfjs_model/$1"
    }
  ],
  "headers": [
    {
      "source": "/tfjs_model/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        }
      ]
    },
    {
      "source": "/(.*\\.json)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.*\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ]
}